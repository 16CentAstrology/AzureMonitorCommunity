{
  "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentParameters.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "alertName": {
      "value": "PureFA Chassis Health"
    },
    "alertDescription": {
      "value": "This alert will trigger when any chassis is unhealthy"
    },
    "alertSeverity": {
      "value": 3
    },
    "metricNamespace": {
      "value": "microsoft.networkcloud/storageappliances"
    },
    "metricName": {
      "value": "PurefaHardwareChassisHealth"
    },
    "operator": {
      "value": "LessThan"
    },
    "threshold": {
      "value": 1
    },
    "targetResourceType": {
      "value": "Microsoft.NetworkCloud/storageAppliances"
    },
    "timeAggregation": {
      "value": "Minimum"
    },
    "windowSize": {
      "value": "PT5M"
    },
    "evaluationFrequency": {
      "value": "PT5M"
    },
    "dimensions": {
      "value": []
    }
  }
}
