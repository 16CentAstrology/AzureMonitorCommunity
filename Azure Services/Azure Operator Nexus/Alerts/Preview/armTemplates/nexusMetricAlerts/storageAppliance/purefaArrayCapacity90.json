{
  "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentParameters.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "alertName": {
      "value": "PureFA Array Space Used - 90 Percent"
    },
    "alertDescription": {
      "value": "This alert will trigger when space usage on array surpasses 90 percent"
    },
    "alertSeverity": {
      "value": 3
    },
    "metricNamespace": {
      "value": "microsoft.networkcloud/storageappliances"
    },
    "metricName": {
      "value": "PurefaArraySpaceUsage"
    },
    "operator": {
      "value": "GreaterThan"
    },
    "threshold": {
      "value": 90
    },
    "targetResourceType": {
      "value": "Microsoft.NetworkCloud/storageAppliances"
    },
    "timeAggregation": {
      "value": "Average"
    },
    "windowSize": {
      "value": "PT5M"
    },
    "evaluationFrequency": {
      "value": "PT5M"
    },
    "dimensions": {
      "value": []
    }
  }
}
